{
  "name": "adiafora/laravel-notification-bitrix24",
  "description": "Package for Laravel notification to Bitrix24",
  "keywords": ["laravel", "Bitrix24", "notification"],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Sergey Zhidkov",
      "email": "zhidkoff@list.ru"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/notifications": "~5.5 || ~6.0 || ~7.0",
    "illuminate/support": "~5.5 || ~6.0 || ~7.0",
    "illuminate/queue": "~5.5 || ~6.0 || ~7.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "~7.0 || ~8.0"
  },
  "autoload": {
    "classmap": [],
    "psr-4": {
      "Adiafora\\Bitrix24\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Adiafora\\Bitrix24\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Adiafora\\Bitrix24\\Bitrix24ServiceProvider"
      ]
    }
  }
}
